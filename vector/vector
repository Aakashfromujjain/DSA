#include<bits/stdc++.h>
using namespace std;

void print(vector<int> &v){
	cout<<"Size: "<<v.size()<<endl;
	for(int i=0;i<v.size();i++){
		cout<<v[i]<<" ";
	}
	v.push_back(18);
	cout<<endl;
}

